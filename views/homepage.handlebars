<h1>Basketball Court Finder</h1>
<h2>Powered by NYC Open Data</h2>
<section id="search-bar">
  <div class="search-container">
    <h1 class="header">Search for basketball-courts in NYC</h1>
    <div class="search-bar">
      <form action="/searchResults" id="search" method="GET">
        <label for="search-input">Search Bar: </label>
        <input type="text" id="search-input" name="searchTerm" placeholder="Type a court name to search for.">
        <button class="search-button" id="search-button" type="submit">seach</button>
      </form>
    </div>
  </div>
</section>
<a href="/user/login" class="nav-item">Login</a>
<a href="/user/register" class="nav-item">Sign Up</a>
<div class="basketball-courts">
<h3>Trending Pick Up Games</h3>
  {{#each trending}}
    <div class="court">
      <h2>{{this.name}}</h2>
      <p>Court Name: {{this.courtName}}</p>
      <p>Location: {{this.location}}</p>
      <p>Date: {{this.date}}</p>
      <a href="game"></a>
    </div>
  {{/each}}
</div>